<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.cafeboy.redis.monitor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" /> <!-- redis -->
    <uses-permission android:name="android.permission.READ_CONTACTS" /> <!-- 获取所有联系人 -->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" /> <!-- 设置头像 -->

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="QRCodeCreatorActivity" />
        <activity android:name="MyContacts" />
        <activity android:name="AndroidExplorer" />
    </application>

</manifest>